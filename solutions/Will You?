function willYou(young, beautiful, loved) {
    if(young){
        if(beautiful){
            if(loved){
                return false;
            } else {
                return true;
            }
        } else {
            if(loved){
                return true;
            } else {
                return false;
            }
        }
    } else {
        if(beautiful){
            if(loved){
                return true;
            } else {
                return false;
            }
        } else {
            if(loved){
                return true;
            } else {
                return false;
            }
        }
    }
}
