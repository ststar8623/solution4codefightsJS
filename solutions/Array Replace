function arrayReplace(inputArray, elemToReplace, substitutionElem) {
    var arr = [];
    for(var i = 0; i < inputArray.length; i++){
        if(inputArray[i] === elemToReplace){
            arr.push(substitutionElem);
        } else {
            arr.push(inputArray[i]);
        }
    }
    return arr;
}
